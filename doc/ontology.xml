        <rdfs:label xml:lang="en-us">dataType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/dependsOn -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/dependsOn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
