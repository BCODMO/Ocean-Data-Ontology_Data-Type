    <!-- http://ocean-data.org/schema/data-type/v1.0/aggregates -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/aggregates">
        <rdfs:domain rdf:resource="http://ocean-data.org/schema/data-type/v1.0/DataType"/>
        <rdfs:range rdf:resource="http://ocean-data.org/schema/data-type/v1.0/DataType"/>
        <rdfs:comment xml:lang="en-us">Aggregates means that the domain DataType is a new DataType that requires
