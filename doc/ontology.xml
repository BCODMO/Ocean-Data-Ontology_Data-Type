        <rdfs:range rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:comment xml:lang="en-us">Specifies that a DataType should include the associated Parameter.</rdfs:comment>
        <rdfs:label xml:lang="en-us">measures</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/recommends -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/recommends">
        <rdfs:subPropertyOf rdf:resource="http://ocean-data.org/schema/data-type/v1.0/measures"/>
        <rdfs:domain rdf:resource="http://ocean-data.org/schema/data-type/v1.0/DataType"/>
        <rdfs:range rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:comment xml:lang="en-us">Specifies that a DataType recommends that the associated Parameter be present,
        but does not require it.</rdfs:comment>
        <rdfs:label xml:lang="en-us">recommends</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/requires -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/requires">
        <rdfs:subPropertyOf rdf:resource="http://ocean-data.org/schema/data-type/v1.0/measures"/>
        <rdfs:domain rdf:resource="http://ocean-data.org/schema/data-type/v1.0/DataType"/>
        <rdfs:range rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:comment xml:lang="en-us">Specifies that a DataType must include the associated Parameter.</rdfs:comment>
        <rdfs:label xml:lang="en-us">requires</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/specializationOf -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/specializationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:range rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:comment xml:lang="en-us">This parameter is narrower version of its range. It a specialization of its range
        by because of some detail such as a specific unit of measure or becuase it was observed
        using a specific instrument.</rdfs:comment>
        <rdfs:label xml:lang="en-us">specializationOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/subjectArea -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/subjectArea">
        <rdfs:domain rdf:resource="http://ocean-data.org/schema/data-type/v1.0/DataType"/>
        <rdfs:range rdf:resource="http://ocean-data.org/schema/data-type/v1.0/SubjectArea"/>
        <rdfs:comment xml:lang="en-us">Specifies that the SubjectArea pertains to the observations made for a DataType.</rdfs:comment>
        <rdfs:label xml:lang="en-us">subjectArea</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/summationOf -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/summationOf">
        <rdfs:subPropertyOf rdf:resource="http://ocean-data.org/schema/data-type/v1.0/dependsOn"/>
        <rdfs:domain rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:range rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:comment xml:lang="en-us">The value of this Parameter is the summation of Parameters attached by this property.</rdfs:comment>
        <rdfs:label xml:lang="en-us">summationOf</rdfs:label>
    </owl:ObjectProperty>
    


