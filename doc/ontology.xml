        <rdfs:range rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:comment xml:lang="en-us">This inferred parameter stores the value of the parameter in range.</rdfs:comment>
        <rdfs:label xml:lang="en-us">inferredParameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/measures -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/measures">
