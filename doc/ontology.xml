        <rdfs:range rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:comment xml:lang="en-us">The defining Parameter is an annotation of the target Parameter</rdfs:comment>
        <rdfs:label xml:lang="en-us">annotationOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/dataType -->


