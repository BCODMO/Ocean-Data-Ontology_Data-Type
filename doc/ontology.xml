        <rdfs:range rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:comment xml:lang="en-us">This parameter depends on the existence on another parameter.</rdfs:comment>
        <rdfs:label xml:lang="en-us">dependsOn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/extends -->


    <owl:ObjectProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/extends">
