    <!-- http://ocean-data.org/schema/data-type/v1.0/dataValueDelimiter -->


    <owl:DatatypeProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/dataValueDelimiter">
        <rdfs:domain rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en-us">For parameters whose data value is a list of multiple values, this property defines a string
        that determines how that value should be parsed into a list of values by specifying the string identifying
        where the value should be split.</rdfs:comment>
        <rdfs:label xml:lang="en-us">delimiter</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ocean-data.org/schema/data-type/v1.0/dataValueFormat -->


    <owl:DatatypeProperty rdf:about="http://ocean-data.org/schema/data-type/v1.0/dataValueFormat">
        <rdfs:domain rdf:resource="http://ocean-data.org/schema/Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en-us">A string, indicating a format for the data value that enables correct processing of the value. For example, a date value may have a format such as &apos;YYYY-MM-DD&apos;. Since the a format specified here are implementation specific, this definition does not enforce any particular range on this property.</rdfs:comment>
        <rdfs:label xml:lang="en-us">dataValueFormat</rdfs:label>
    </owl:DatatypeProperty>
    


