                                                <http://purl.org/dc/elements/1.1/creator> "Adam Shepherd" ;
                                                <http://purl.org/dc/elements/1.1/publisher> "Biological and Chemical Oceanography Data Management Office (BCO-DMO)" ;
                                                <http://purl.org/dc/terms/creator> <http://orcid.org/0000-0003-4486-9448> ;
                                                <http://purl.org/dc/terms/issued> "2017-09-04"^^xsd:date ;
                                                <http://purl.org/dc/terms/publisher> <http://lod.bco-dmo.org/id/affiliation/191> ;
                                                <http://purl.org/dc/terms/source> "https://github.com/BCODMO/Ocean-Data-Ontology_Data-Type"^^xsd:anyURI ;
                                                rdfs:comment "An ontology for describing data types used by BCO-DMO"@en-us ;
                                                rdfs:label "Ocean Data Ontology - Data Type Extension"@en-us ;
                                                rdfs:seeAlso <http://info.deepcarbon.net/schema#DataType> ;
                                                owl:versionInfo "1.0" .

