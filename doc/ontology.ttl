#    Object Properties
#################################################################

###  http://ocean-data.org/schema/archiver
<http://ocean-data.org/schema/archiver> rdf:type owl:ObjectProperty ,
                                                 owl:FunctionalProperty ;
                                        rdfs:domain <http://ocean-data.org/schema/ArchiveRecord> ;
                                        rdfs:range <http://ocean-data.org/schema/Organization> ;
                                        rdfs:comment "The Organization acting as the archiver of some research product."@en-us ;
                                        rdfs:label "archiver"@en-us .


###  http://ocean-data.org/schema/archives
<http://ocean-data.org/schema/archives> rdf:type owl:ObjectProperty ;
                                        owl:inverseOf <http://ocean-data.org/schema/isArchivedBy> ;
                                        rdf:type owl:FunctionalProperty ;
                                        rdfs:domain <http://ocean-data.org/schema/ArchiveRecord> ;
                                        rdfs:range <http://ocean-data.org/schema/CreativeWork> ;
                                        rdfs:comment "The CreativeWork archived by some Organization described in this record."@en-us ;
                                        rdfs:label "archives"@en-us .


###  http://ocean-data.org/schema/checksumAlgorithm
<http://ocean-data.org/schema/checksumAlgorithm> rdf:type owl:ObjectProperty ;
                                                 rdfs:domain <http://ocean-data.org/schema/DigitalDocument> ;
                                                 rdfs:range owl:Thing ;
                                                 rdfs:comment "The algorithm of some cryptographic hash"@en-us ;
                                                 rdfs:label "checksumAlgorithm"@en-us .


###  http://ocean-data.org/schema/crossrefRelation
<http://ocean-data.org/schema/crossrefRelation> rdf:type owl:ObjectProperty ;
                                                rdfs:domain <http://ocean-data.org/schema/CreativeWorkRelationType> ;
                                                rdfs:range <http://ocean-data.org/schema/CrossrefRelation> ;
                                                rdfs:comment "The type of relationship defined by Crossref."@en-us ;
                                                rdfs:label "crossrefRelation"@en-us .


###  http://ocean-data.org/schema/currentState
<http://ocean-data.org/schema/currentState> rdf:type owl:ObjectProperty ,
                                                     owl:FunctionalProperty ;
                                            rdfs:domain <http://ocean-data.org/schema/Dataset> ;
                                            rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#member> ;
                                                                                owl:allValuesFrom <http://ocean-data.org/schema/CurrentState>
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ] ;
                                            rdfs:comment "A term describing the current state of the dataset."@en-us ;
                                            rdfs:label "currentState"@en-us .


###  http://ocean-data.org/schema/dataFile
<http://ocean-data.org/schema/dataFile> rdf:type owl:ObjectProperty ;
                                        owl:inverseOf <http://ocean-data.org/schema/serializes> ;
                                        rdfs:domain <http://ocean-data.org/schema/Dataset> ;
                                        rdfs:range <http://ocean-data.org/schema/DataFile> ;
                                        rdfs:comment "A data file recording the observations and measurements for a dataset."@en-us ;
                                        rdfs:label "dataFile"@en-us .


###  http://ocean-data.org/schema/datasetType
<http://ocean-data.org/schema/datasetType> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <http://ocean-data.org/schema/Dataset> ;
                                           rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty <http://www.w3.org/2004/02/skos/core#member> ;
                                                                               owl:allValuesFrom <http://ocean-data.org/schema/DatasetType>
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ] ;
                                           rdfs:comment "The type of dataset."@en-us ;
                                           rdfs:label "datasetType"@en-us .


###  http://ocean-data.org/schema/forOrganization
<http://ocean-data.org/schema/forOrganization> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <http://ocean-data.org/schema/Role> ;
                                               rdfs:range <http://ocean-data.org/schema/Organization> .


###  http://ocean-data.org/schema/fromCollection
<http://ocean-data.org/schema/fromCollection> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <http://ocean-data.org/schema/hasInventory> ;
                                              rdfs:domain <http://ocean-data.org/schema/Dataset> ;
                                              rdfs:range <http://ocean-data.org/schema/DatasetCollection> .


###  http://ocean-data.org/schema/fromDeployment
<http://ocean-data.org/schema/fromDeployment> rdf:type owl:ObjectProperty ,
                                                       owl:FunctionalProperty ;
                                              rdfs:domain <http://ocean-data.org/schema/DeploymentDataset> ;
                                              rdfs:range <http://ocean-data.org/schema/Deployment> ;
                                              rdfs:comment " The deployment from which this dataset was collected."@en-us ;
                                              rdfs:label "fromDeployment"@en-us .


###  http://ocean-data.org/schema/fromInstrument
<http://ocean-data.org/schema/fromInstrument> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://ocean-data.org/schema/Dataset> ;
                                              rdfs:range <http://ocean-data.org/schema/Instrument> ;
                                              rdfs:comment "This dataset was collected from an instrument that is located on the platform during this deployment."@en-us ;
                                              rdfs:label "fromInstrument"@en-us .


###  http://ocean-data.org/schema/funded
<http://ocean-data.org/schema/funded> rdf:type owl:ObjectProperty ;
                                      owl:inverseOf <http://ocean-data.org/schema/fundedBy> ;
                                      rdfs:domain <http://ocean-data.org/schema/Award> ;
                                      rdfs:range [ rdf:type owl:Class ;
                                                   owl:unionOf ( <http://ocean-data.org/schema/CreativeWork>
                                                                 <http://ocean-data.org/schema/ResearchActivity>
                                                               )
                                                 ] ;
                                      rdfs:comment "Funded the research actvity or product."@en-us ;
                                      rdfs:label "funded"@en-us .


###  http://ocean-data.org/schema/fundedBy
<http://ocean-data.org/schema/fundedBy> rdf:type owl:ObjectProperty ;
                                        rdfs:domain [ rdf:type owl:Class ;
                                                      owl:unionOf ( <http://ocean-data.org/schema/CreativeWork>
                                                                    <http://ocean-data.org/schema/ResearchActivity>
                                                                  )
                                                    ] ;
                                        rdfs:range <http://ocean-data.org/schema/Award> ;
                                        rdfs:comment "Specifies how a research activity or product was funded"@en-us ;
                                        rdfs:label "fundedBy"@en-us .


###  http://ocean-data.org/schema/generatedWork
<http://ocean-data.org/schema/generatedWork> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <http://www.w3.org/ns/prov#generated> ;
                                             rdfs:domain <http://ocean-data.org/schema/ResearchActivity> ;
                                             rdfs:range <http://ocean-data.org/schema/CreativeWork> ;
                                             rdfs:comment "A creative work, research product, that was generated due to some research activity."@en-us ;
                                             rdfs:label "generatedWork"@en-us .


###  http://ocean-data.org/schema/hasAgentWithRole
<http://ocean-data.org/schema/hasAgentWithRole> rdf:type owl:ObjectProperty ;
                                                owl:inverseOf <http://ocean-data.org/schema/roleAbout> ;
                                                rdfs:domain [ rdf:type owl:Class ;
                                                              owl:unionOf ( <http://ocean-data.org/schema/Award>
                                                                            <http://ocean-data.org/schema/Dataset>
                                                                            <http://ocean-data.org/schema/Deployment>
                                                                            <http://ocean-data.org/schema/Organization>
                                                                            <http://ocean-data.org/schema/ResearchActivity>
                                                                          )
                                                            ] ;
                                                rdfs:range <http://ocean-data.org/schema/Role> ;
                                                rdfs:comment "Specifies role some agent played on a certain research activity or product."@en-us ;
                                                rdfs:label "hasAgentWithRole"@en-us .


###  http://ocean-data.org/schema/hasAuthoritativePublisher
<http://ocean-data.org/schema/hasAuthoritativePublisher> rdf:type owl:ObjectProperty ;
                                                         rdfs:domain <http://ocean-data.org/schema/CreativeWork> ;
                                                         rdfs:range <http://ocean-data.org/schema/Agent> ;
                                                         rdfs:comment "The Agent that is the authoritative source for some Creative Work. Was using dcterms:publisher."@en-us ;
                                                         rdfs:label "hasAuthoritativePublisher"@en-us .


###  http://ocean-data.org/schema/hasAward
<http://ocean-data.org/schema/hasAward> rdf:type owl:ObjectProperty ;
                                        rdfs:domain [ rdf:type owl:Class ;
                                                      owl:unionOf ( <http://ocean-data.org/schema/Dataset>
                                                                    <http://ocean-data.org/schema/FundingSource>
                                                                    <http://ocean-data.org/schema/Project>
                                                                  )
                                                    ] ;
                                        rdfs:range <http://ocean-data.org/schema/Award> ;
                                        rdfs:comment "Whether an activity (Project) or Entity (Dataset or Funding Source) is attached to an award. This should probably be brokenn into different terms - :Dataset 'fundedBy' :Award; :FundingSource 'provesAward' :Award; :Award 'fundsActivity' :Project"@en-us ;
                                        rdfs:label "hasAward"@en-us ;
                                        owl:deprecated "true"^^xsd:boolean ;
                                        <http://www.w3.org/2004/02/skos/core#related> <http://ocean-data.org/schema/fundedBy> ,
                                                                                      <http://ocean-data.org/schema/providesAward> .


###  http://ocean-data.org/schema/hasCoordinatedDeployment
<http://ocean-data.org/schema/hasCoordinatedDeployment> rdf:type owl:ObjectProperty ;
                                                        rdfs:domain <http://ocean-data.org/schema/Deployment> ;
                                                        rdfs:range <http://ocean-data.org/schema/Deployment> ;
                                                        rdfs:comment "A disparate deployment that coordinated in conjuction with this subject for data collection"@en-us ;
                                                        rdfs:label "hasCoordinatedDeployment"@en-us .


###  http://ocean-data.org/schema/hasCruiseTrajectory
<http://ocean-data.org/schema/hasCruiseTrajectory> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <http://ocean-data.org/schema/Cruise> ;
                                                   rdfs:range <http://www.opengis.net/ont/geosparql#Feature> ;
                                                   rdfs:comment "The set of ordered coordinates forming a geometry (usually MultiPoint) representing the path taken by a Cruise."@en-us ;
                                                   rdfs:label "hasCruiseTrajectory"@en-us .


###  http://ocean-data.org/schema/hasDataset
<http://ocean-data.org/schema/hasDataset> rdf:type owl:ObjectProperty ;
                                          rdfs:domain [ rdf:type owl:Class ;
                                                        owl:unionOf ( <http://ocean-data.org/schema/Award>
                                                                      <http://ocean-data.org/schema/DeploymentDataset>
                                                                      <http://ocean-data.org/schema/Project>
                                                                    )
                                                      ] ;
                                          rdfs:range <http://ocean-data.org/schema/Dataset> .


###  http://ocean-data.org/schema/hasDeploymentDates
<http://ocean-data.org/schema/hasDeploymentDates> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain <http://ocean-data.org/schema/Deployment> ;
                                                  rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
                                                  rdfs:comment "A date range for the start and end of a deployment"@en-us ;
                                                  rdfs:label "hasDeploymentDates"@en-us .


###  http://ocean-data.org/schema/hasFundingSource
<http://ocean-data.org/schema/hasFundingSource> rdf:type owl:ObjectProperty ;
                                                rdfs:domain <http://ocean-data.org/schema/Award> ;
                                                rdfs:range <http://ocean-data.org/schema/Organization> ;
                                                rdfs:comment "The source of funding for an award"@en-us ;
                                                rdfs:label "hasFundingSource"@en-us .


###  http://ocean-data.org/schema/hasIdentifier
<http://ocean-data.org/schema/hasIdentifier> rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://ocean-data.org/schema/CreativeWork> ;
                                             rdfs:range <http://ocean-data.org/schema/Identifier> ;
                                             rdfs:comment "An identifier for a Creative Work."@en-us .


###  http://ocean-data.org/schema/hasInventory
<http://ocean-data.org/schema/hasInventory> rdf:type owl:ObjectProperty ;
                                            rdfs:domain <http://ocean-data.org/schema/DatasetCollection> ;
                                            rdfs:range <http://ocean-data.org/schema/Dataset> ;
                                            rdfs:comment "Defines the Datasets that are part of this collection"@en-us ,
                                                         "fix" ;
                                            rdfs:label "hasInventory"@en-us .


###  http://ocean-data.org/schema/hasParentProgram
<http://ocean-data.org/schema/hasParentProgram> rdf:type owl:ObjectProperty ;
