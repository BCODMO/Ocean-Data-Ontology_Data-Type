#### Ontology

<http://ocean-data.org/schema/data-type/v1.0> a owl:Ontology ;
    rdfs:label "Ocean Data Ontology - Data Types Schema"@en-US ;
    rdfs:comment "An ontology for describing data types used by BCO-DMO"@en-US ;
    rdfs:seeAlso <http://info.deepcarbon.net/schema#DataType> ;
    dc:creator "Adam Shepherd" ;  
    dct:creator <http://orcid.org/0000-0003-4486-9448> ;  
    dc:publisher "Biological and Chemical Oceanography Data Management Office (BCO-DMO)" ;
    dct:publisher <http://lod.bco-dmo.org/id/affiliation/191> ;
    dct:issued "2017-09-04"^^xsd:date ;
    dct:source "https://github.com/BCODMO/Ocean-Data-Ontology_Data-Type"^^xsd:anyURI ;
    owl:imports <http://ocean-data.org/schema/> ;
    owl:versionInfo "0.1.0" .


#### Annotation Properties 

dc:creator a owl:AnnotationProperty .
dct:creator a owl:AnnotationProperty .
dc:publisher a owl:AnnotationProperty .
dct:publisher a owl:AnnotationProperty .
dct:issued a owl:AnnotationProperty .
dct:source a owl:AnnotationProperty .
prov:specializationOf a owl:AnnotationProperty .
prov:wasInfluencedBy a owl:AnnotationProperty .


#### Classes

odo-dt:DataType a owl:Class ;
    rdfs:label "DataType"@en-us ;
    rdfs:comment """A data type can include more meaning beyond the syntax, such as who created the
  data type, the source standard that the data type derives from,
  the operations that can be done on datasets of that data type, and
  typical scientific domains, software programs and/or instruments
  that use the data type, etc. The intent is that any humans or
  machines that when they encounter that data type can quickly
  'understand' or be in a situation to at least 'process' details
  within the dataset without even downloading that dataset."""@en-US ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onProperty odo:aggregates ] .

odo-dt:SubjectArea a owl:Class ;
    rdfs:label "SubjectArea"@en-us ;
    rdfs:comment "A certain field of study used to group or filter Data Types"@en-us .

# Make odo:Parameter a prov:Entity so that it can reuse prov:specializationOf
odo:Parameter a prov:Entity .

#### Object Properties

odo-dt:dataType a owl:ObjectProperty ;
    rdfs:label "dataType"@en-us ;
    rdfs:comment "Specifies that a Dataset meets the criteria for a certain DataType."@en-us ;
    rdfs:domain odo:Dataset ;
    rdfs:range odo-dt:DataType .

odo-dt:subjectArea a owl:ObjectProperty ;
    rdfs:label "subjectArea"@en-us ;
    rdfs:comment "Specifies that the SubjectArea pertains to the observations made for a DataType."@en-us ;
    rdfs:domain odo-dt:DataType ;
    rdfs:range odo-dt:SubjectArea .
    
odo-dt:extends a owl:ObjectProperty ;
    rdfs:label "extends"@en-us ;
    rdfs:comment "Extends means that the domain DataType is a specialization of the range DataType by inheriting the range's Parameters but also incluing new Parameters as part of the specialization."@en-us ;
    rdfs:domain odo-dt:DataType ;
    rdfs:range odo-dt:DataType .
    
odo-dt:aggregates a owl:ObjectProperty ;
    rdfs:label "aggregates"@en-us ;
    rdfs:comment "Aggregates means that the domain DataType is a new DataType that requires at least one instance of the aggregates range."@en-us ;
    rdfs:domain odo-dt:DataType ;
    rdfs:range odo-dt:DataType .

odo-dt:measures a owl:ObjectProperty ;
    rdfs:label "measures"@en-us ;
    rdfs:comment "Specifies that a DataType should include the associated Parameter."@en-us ;
    rdfs:domain odo-dt:DataType ;
    rdfs:range odo:Parameter .
    
odo-dt:requires a owl:ObjectProperty ;
    rdfs:label "requires"@en-us ;
    rdfs:comment "Specifies that a DataType must include the associated Parameter."@en-us ;
    rdfs:domain odo-dt:DataType ;
    rdfs:range odo:Parameter ;
    rdfs:subPropertyOf odo-dt:measures .
    
odo-dt:specializationOf a owl:ObjectProperty ;
    rdfs:label "specializationOf"@en-us ;
    rdfs:comment "This parameter is narrower version of its range. It a specialization of its range by because of some detail such as a specific unit of measure or becuase it was observed using a specific instrument."@en-us ;
    rdfs:domain odo:Parameter ;
    rdfs:range odo:Parameter ;
    rdfs:subPropertyOf prov:specializationOf .

