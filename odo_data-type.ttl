@prefix : <http://ocean-data.org/schema/data-type/v1.0/> .
@prefix odo-dt: <http://ocean-data.org/schema/data-type/v1.0/> .
@prefix odo: <http://ocean-data.org/schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
