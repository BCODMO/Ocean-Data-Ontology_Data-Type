odo-dt:TotalIonChromatogram a owl:NamedIndividual,
        odo:Parameter ;
    rdfs:label "Total Ion Chromatogram"@en ;
    rdfs:comment "Total Ion Chromatogram"@en .

odo-dt:Plus2HSpectraCount a owl:NamedIndividual,
        odo:Parameter ;
    rdfs:label "Number of identified +2H spectra"@en ;
    rdfs:comment "Number of identified +2H spectra"@en .

odo-dt:Plus3HSpectraCount a owl:NamedIndividual,
        odo:Parameter ;
    rdfs:label "Number of identified +3H spectra"@en ;
    rdfs:comment "Number of identified +3H spectra"@en .

odo-dt:Plus4HSpectraCount a owl:NamedIndividual,
        odo:Parameter ;
    rdfs:label "Number of identified +4H spectra"@en ;
    rdfs:comment "Number of identified +4H spectra"@en .

odo-dt:AbsoluteUnits_FemtomolePerLiter a owl:NamedIndividual,
        odo:Parameter ;
    rdfs:label "Absolute Units in femtomole per liter"@en ;
    rdfs:comment "Absolute Units in femtomole per liter"@en .

odo-dt:PeptideSpectralCounts_ProteinIdentifier a owl:NamedIndividual,
        odo:Parameter ;
    ## Axiom:
    ## if a odo-dt:OceanProteinPortalDataType,
    ## and odo-dt:PepetideSpectralCounts odo-dt:measures odo-dt:PeptideSpectralCounts_ProteinIdentifier ;
    ## then, odo-dt:PeptideSpectralCounts_ProteinIdentifier owl:allValuesFrom ( odo-dt:ProteinSpectralCounts odo-dt:measures odo-dt:ProteinIdentifier ).
    ## 
    prov:wasInfluencedBy odo-dt:ProteinIdentifier ;
    rdfs:label "Peptide Protein Identifier"@en ;
    rdfs:comment "Representative of the protein from which this peptide came from."@en .

odo-dt:PeptideSpectralCounts_ProteinAccessionId a owl:NamedIndividual,
        odo:Parameter ;
    ## Axiom:
    ## if a odo-dt:OceanProteinPortalDataType,
    ## and odo-dt:PepetideSpectralCounts odo-dt:measures odo-dt:PeptideSpectralCounts_ProteinAccessionId ;
    ## then, odo-dt:PeptideSpectralCounts_ProteinAccessionId owl:allValuesFrom ( odo-dt:ProteinSpectralCounts odo-dt:measures odo-dt:ProteinIdentifier ).
    ## 
    prov:wasInfluencedBy odo-dt:ProteinIdentifier ;
    skos:related odo-dt:PeptideSpectralCounts_ProteinAccessionId ;
    rdfs:label "Protein Accession Identifier"@en ;
    rdfs:comment "Representative of all the possible proteins from which this peptide came from."@en .

odo-dt:PeptideSpectralCounts_StartIndex a owl:NamedIndividual,
        odo:Parameter ;
    rdfs:label "Peptide start index"@en ;
    rdfs:comment "Data useful in creating peptide coverage map"@en .

odo-dt:PeptideSpectralCounts_StopIndex a owl:NamedIndividual,
        odo:Parameter ;
    prov:wasInfluencedBy odo-dt:PeptideSpectralCounts_StartIndex ;
    rdfs:label "Peptide stop index"@en ;
    rdfs:comment "Data useful in creating peptide coverage map"@en .
