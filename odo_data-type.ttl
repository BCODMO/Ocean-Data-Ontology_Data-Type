        owl:NamedIndividual ;
    rdfs:label "Tabular File of Protein Spectral Counts"@en-us .
    
odo-dt:OceanProteinPortalDataType a odo-dt:DataType,
        owl:NamedIndividual ;
    rdfs:label "Ocean Protein Portal Data Type"@en-us ;
    odo-dt:aggregates odo-dt:FASTA-ProteinIdentifications,
        odo-dt:PeptideSpectralCounts,
        odo-dt:ProteinSpectralCounts ;
    odo-dt:requires odo-dt:CruiseIdentifier,
        odo-dt:CruiseStationIdentifier,
        odo-dt:CruiseStationLatitudeDecimalDegrees,
        odo-dt:CruiseStationLongitudeDecimalDegrees,
    
odo-dt:Ocean-Proteomics a odo-dt:SubjectArea,
    owl:NamedIndividual ;
    skos:broader odo-dt:Proteomics ;
    rdfs:label "Ocean Proteomics"@en-us .

odo-dt:Proteomics a odo-dt:SubjectArea,
