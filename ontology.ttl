#### Axioms
[] a owl:Restriction ;
    rdfs:subClassOf odo-dt:DataType ;
    owl:onProperty odo-dt:aggregates ;
    owl:minCardinality "2"^^xsd:nonNegativeInteger .

[] a owl:Restriction ;
    rdfs:subClassOf odo:Parameter ;
    owl:onProperty odo-dt:summationOf ;
    owl:minCardinality "2"^^xsd:nonNegativeInteger .

[] a owl:Restriction ;
    rdfs:subClassOf odo-dt:InferredParameter ;
    owl:onProperty odo-dt:inferredValueTarget ;
    owl:cardinality "1"^^xsd:nonNegativeInteger .

[] a owl:Restriction ;
    rdfs:subClassOf odo-dt:InferredParameter ;
    owl:onProperty odo-dt:inferredValueTargetProperty ;
    owl:cardinality "1"^^xsd:nonNegativeInteger .

# Trying to say the the dc:title field of an odo:Cruise connected to an odo:Dataset through odo:fromDeployment is the
# value of an inferred odo-dt:CruiseIdentifier
odo-dt:datasetCruiseIdentifier a owl:DatatypeProperty ;
    owl:propertyChainAxiom odo-dt:hasCruiseIdentifierPath ;
    odo-dt:inferredParameter odo-dt:CruiseIdentifier ;
    rdfs:label "datasetCruiseIdentifier"@en-US ;
    rdfs:comment "Specifies the inferred Cruise Identifier(s) based on the dataset metadata."@en-US ;
    rdfs:domain odo:Dataset ;
    rdfs:range xsd:string .   
[] a owl:Axiom ;
    owl:annotatedSource odo-dt:hasCruiseIdentifierPath ;
    owl:annotatedProperty owl:propertyChainAxiom ;
    owl:annotatedTarget odo-dt:cruiseIdentifierProperty ;
    rdfs:comment "A cruise's title is the inferred value of the 'CruiseIdentifier' parameter for a dataset."@en-US .
odo-dt:hasCruiseIdentifierPath owl:propertyChainAxiom [
        rdf:first odo:fromDeployment ;
        rdf:rest odo-dt:cruiseIdentifierProperty 
    ] .
odo-dt:cruiseIdentifierProperty 
    rdf:first dc:title ;
    rdf:rest rdf:nil .
